progname=dvipdf
CC=@CC@
INSTALL=@INSTALL@
CFLAGS += @CFLAGS@  @DEFS@ -I../kpathsea/
LIBDIR = ../kpathsea/kpathsea/STATIC
INCDIR = ../kpathsea/
prefix = @prefix@
exec_prefix = @exec_prefix@
BINDIR = @bindir@
HEADERS = acdefines.h dvi.h pdflimits.h error.h io.h jpeg.h mem.h numbers.h \
	pdfdev.h pdfdoc.h pdfparse.h pdfspecial.h tfm.h type1.h
SRC = pdfdev.c pdfdoc.c dvi.c io.c mem.c jpeg.c numbers.c pdfobj.c \
	pdfparse.c pdfspecial.c tfm.c type1.c dvipdf.c
OBJ = pdfdev.o pdfdoc.o dvi.o io.o mem.o jpeg.o numbers.o pdfobj.o \
        pdfparse.o pdfspecial.o tfm.o type1.o dvipdf.o
CONFIG = config.cache config.status Makefile config.h config.log
PROGRAMS = dvipdfm
default: all
all:	$(PROGRAMS)
dvipdfm: $(OBJ)
	$(CC) $(CFLAGS) -o dvipdfm $(OBJ) -lkpathsea -lm -L$(LIBDIR)
install: dvipdfm
	$(INSTALL) dvipdfm $(BINDIR)
distclean: clean
	rm -f $(CONFIG)
clean:
	rm -f $(OBJ)
	rm -f $(PROGRAMS)
$(OBJ): $(HEADERS)	

