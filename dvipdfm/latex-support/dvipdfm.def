\def\Gin@extensions{.jpg,.jpeg,.pdf,.png}
\@namedef{Gin@rule@.jpg}#1{{bmp}{.bb}{#1}}
\@namedef{Gin@rule@.jpeg}#1{{bmp}{.bb}{#1}}
\@namedef{Gin@rule@.png}#1{{bmp}{.bb}{#1}}
\@namedef{Gin@rule@.pdf}#1{{pdf}{.bb}{#1}}
\@namedef{Gin@rule@*}#1{{eps}{\Gin@ext}{#1}}
